<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Semana3Navegacion.MainPage">

    <StackLayout>
        <Label Text="Esta es la ventan 1" TextColor="Blue">
            
        </Label>
        <Entry x:Name="txtuser"
               Placeholder="Ingrese el usuario"
               ></Entry>

        <Entry x:Name="txtpass"
               Placeholder="Ingrese la clave"
               IsPassword="True">
         </Entry>
        <Button x:Name="btnAbrir_Venta_dos"
                Text="Abrir ventana dos"
                Clicked="btnAbrir_Venta_dos_Clicked">
            
        </Button>
    </StackLayout>

</ContentPage>
